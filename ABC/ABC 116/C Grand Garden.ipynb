    "import numpy as np\n",
    "n=8\n",
    "h=[4,23,75,0,23,96,50,100]\n",
    "a=np.zeros((n+1,100))\n",
    "for i in range(n):\n",
    "    for k in range(h[i]):\n",
    "        a[i][k]=1\n",
    "c=0\n",
    "for i in range(100):\n",
    "    for k in range(n):\n",
    "        if a[k+1][i]-a[k][i]==-1:\n",
    "            c += 1\n",
    "print(a[0])\n",
    "print(c)"
