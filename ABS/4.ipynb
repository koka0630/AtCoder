    "N=int(input())\n",
    "A=list(map(int,input().split()))\n",
    "B=[0]*N\n",
    "for i in range(N):\n",
    "    a=0\n",
    "    b=A[i]\n",
    "    while b%2==0:\n",
    "        a+=1\n",
    "        b=b/2\n",
    "    B[i]=a\n",
    "print(min(B))"
